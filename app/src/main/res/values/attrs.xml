<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HighlightView">
        <attr name="startColor" format="color"/>
        <attr name="endColor" format="color"/>
        <attr name="frameColor" format="color"/>
        <attr name="frameWidth" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="CircleLoader">
        <attr name="ringWidth" format="dimension"/>
        <attr name="rollerWidth" format="dimension"/>
        <attr name="ringColor" format="color"/>
        <attr name="rollerColor" format="color"/>
        <attr name="rollerSweepAngle" format="float"/>
        <attr name="animDuration" format="integer"/>
    </declare-styleable>
</resources>